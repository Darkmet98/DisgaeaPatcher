{
  "executable": "DisgaeaPatcher",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "DisgaeaPatcher",
  "author": "HyperTraducciones",
  "singleInstance": true,
  "environment": "Production",
  "build": {
    "appId": "com.DisgaeaPatcher.app",
    "productName": "DisgaeaPatcher",
    "copyright": "Copyright HyperTraducciones Â© 2020",
	"icon": "../../../icon.ico",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}